{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-standard",
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"member-access": [
			true,
			"no-public"
		],
		"interface-over-type-literal": false,
		"object-literal-sort-keys": false,
		"member-ordering": false,
		"jsx-no-lambda": false,
		"jsx-boolean-value": false
	},
	"rulesDirectory": []
}